# Server
HTTP_PORT=8000
LOG_LEVEL=INFO

# Database
DB_NAME=techyouth.db

# JWT
JWT_ACCESS_SECRET=your_access_secret_here
JWT_REFRESH_SECRET=your_refresh_secret_here
JWT_OAUTH_SECRET=fallback_oauth_secret  # if needed

# Email (if you're sending verification emails)
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASSWORD=
FROM_EMAIL=

# Admin
INTERNAL_PASSWORD=your_internal_password

# CORS (if needed)
ALLOWED_ORIGINS=*

# Redis (if using)
REDIS_HOST=localhost
REDIS_PORT=6379

# GitHub Personal Access Token (for private repositories)
# Generate token at: https://github.com/settings/tokens
# Required scope: repo (Full control of private repositories)
GITHUB_TOKEN=ghp_your_token_here

# Docker Compose Database Configuration
POSTGRES_USER=ven_user
POSTGRES_PASSWORD=ven_password
POSTGRES_DB=ven_api

# Docker Compose Redis Configuration
REDIS_PASSWORD=ven-redis-pass

# Docker Compose MinIO Configuration
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=password123